{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://example.com/show.schema.json",
	"title": "Show",
	"type": "object",
	"properties": {
		"showId": {
			"type": "integer"
		},
		"filmId": {
			"type": "integer"
		},
		"startTime": {
			"type": "string"
		},
		"finishTime": {
			"type": "string"
		},
		"hall": {
			"enum": [ 1, 2, 3 ]
		}
	},

	"required": [ "showId", "filmId", "startTime", "finishTime", "hall" ]

}